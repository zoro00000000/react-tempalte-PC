{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],      // 支持es7装饰器
        "@babel/plugin-syntax-dynamic-import",      // code splitting 按需加载
        "react-hot-loader/babel",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css" // `style: true` 会加载 less 文件
        }]
    ]
}